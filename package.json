{
  "name": "urara",
  "type": "module",
  "version": "0.0.1",
  "license": "WTFPL",
  "repository": "importantimport/urara",
  "homepage": "https://github.com/importantimport/urara",
  "bugs": "https://github.com/importantimport/urara/issues",
  "author": "Ëóç+85CD",
  "scripts": {
    "clean": "node urara.js clean",
    "tsc": "tsc -p tsconfig.node.json",
    "tsc:watch": "tsc -w -p tsconfig.node.json",
    "urara:build": "node urara.js build",
    "urara:watch": "node urara.js watch",
    "kit:dev": "export NODE_OPTIONS=--max_old_space_size=7680 && MODE=development svelte-kit dev",
    "kit:build": "export NODE_OPTIONS=--max_old_space_size=7680 && svelte-kit build",
    "dev:parallel": "npm-run-all -p -r tsc:watch urara:watch \"kit:dev {@} \" --",
    "dev": "npm-run-all -s tsc \"dev:parallel {@} \" --",
    "build": "npm-run-all -s tsc urara:build kit:build clean",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@iconify-json/heroicons-outline": "^1.1.2",
    "@iconify-json/heroicons-solid": "^1.1.2",
    "@sveltejs/adapter-auto": "1.0.0-next.50",
    "@sveltejs/adapter-node": "1.0.0-next.78",
    "@sveltejs/adapter-static": "1.0.0-next.34",
    "@sveltejs/kit": "1.0.0-next.350",
    "@tailwindcss/typography": "^0.5.2",
    "@types/node": "^18.0.0",
    "@types/tailwindcss": "^3.0.11",
    "@types/unist": "^2.0.6",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "autoprefixer": "^10.4.7",
    "chalk": "^5.0.1",
    "chokidar": "^3.5.3",
    "cssnano": "^5.1.12",
    "daisyui": "^2.17.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "fenceparser": "^2.0.0",
    "github-slugger": "^1.4.0",
    "mdast-util-to-string": "^3.1.0",
    "mdsvex": "^0.10.6",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-external-links": "^1.0.1",
    "rehype-slug": "^5.0.1",
    "remark": "^14.0.2",
    "remark-footnotes": "~2.0.0",
    "shiki-twoslash": "^3.0.2",
    "svelte": "^3.48.0",
    "svelte-check": "^2.8.0",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.1.4",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "unist-util-visit": "^4.1.0",
    "unocss": "^0.42.0",
    "vite": "^2.9.13",
    "vite-plugin-pwa": "^0.12.2",
    "workbox-window": "^6.5.3"
  }
}